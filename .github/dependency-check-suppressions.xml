<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
        BUTTERFLY Dependency Check Suppressions
        
        This file contains false positive suppressions for the OWASP Dependency Check.
        Each suppression should include:
        - A clear comment explaining why it's suppressed
        - The CVE or vulnerability ID being suppressed
        - The affected package
        - When it should be reviewed again
        
        DO NOT suppress actual vulnerabilities without proper security review.
    -->
    
    <!-- Example suppression format (uncomment and customize as needed):
    <suppress>
        <notes><![CDATA[
        file name: example-lib-1.0.jar
        Reason: This is a false positive. The vulnerable code path is not used in our application.
        Review by: 2025-06-01
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.example/example-lib@.*$</packageUrl>
        <cve>CVE-2024-XXXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress test-only dependencies that don't affect production -->
    <suppress>
        <notes><![CDATA[
        Test-only dependencies - H2 database is only used in test scope and not deployed to production.
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
</suppressions>

